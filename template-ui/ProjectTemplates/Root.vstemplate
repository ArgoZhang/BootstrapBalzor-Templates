<VSTemplate Version="3.0.0" Type="ProjectGroup" xmlns="http://schemas.microsoft.com/developer/vstemplate/2005">
    <TemplateData>
        <Name>Bootstrap Blazor Project (Server-Side, Wasm)</Name>
        <Description>Project templates for creating Bootstrap Blazor apps that run on the server in an ASP.NET Core app or in the browser on WebAssembly (wasm). These templates can be used to build web apps rich dynamic user interface (UIs).</Description>
        <ProjectType>CSharp</ProjectType>
        <ProjectSubType>Web</ProjectSubType>
        <TemplateGroupID>Web</TemplateGroupID>
        <SortOrder>1</SortOrder>
        <CreateNewFolder>true</CreateNewFolder>
        <DefaultName>BootstrapBlazorApp</DefaultName>
        <ProvideDefaultName>true</ProvideDefaultName>
        <LocationField>Enabled</LocationField>
        <EnableLocationBrowseButton>true</EnableLocationBrowseButton>
        <Icon>project-icon.png</Icon>
        <LanguageTag>csharp</LanguageTag>
        <PlatformTag>linux</PlatformTag>
        <PlatformTag>macos</PlatformTag>
        <PlatformTag>windows</PlatformTag>
        <ProjectTypeTag>cloud</ProjectTypeTag>
        <ProjectTypeTag>web</ProjectTypeTag>
    </TemplateData>
    <TemplateContent>
        <ProjectCollection>
            <ProjectTemplateLink ProjectName="$safeprojectname$.Server" CopyParameters="true">
                BootstrapBlazorApp.Server\MyTemplate.vstemplate
            </ProjectTemplateLink>
            <ProjectTemplateLink ProjectName="$safeprojectname$.WebAssembly" CopyParameters="true">
                BootstrapBlazorApp.WebAssembly\MyTemplate.vstemplate
            </ProjectTemplateLink>
            <ProjectTemplateLink ProjectName="$safeprojectname$.Shared" CopyParameters="true">
                BootstrapBlazorApp.Shared\MyTemplate.vstemplate
            </ProjectTemplateLink>
        </ProjectCollection>
    </TemplateContent>
</VSTemplate>
